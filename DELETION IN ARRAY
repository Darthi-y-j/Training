#include <stdio.h>

int main()
{
    int size,i,position,value;
printf("position");//2
scanf("%d",&position);
printf("size");
scanf("%d",&size);
// printf("value");
// scanf("%d",&value);
int arr[100];
printf("enter the array values");
for(i=0;i<size;i++)
{
    scanf("%d",&arr[i]);
}

for(i=position;i<=size;i++)
{
    arr[i-1]=arr[i];
}
for(i=0;i<size-1;i++)
printf("\t%d",arr[i]);
    return 0;
}
