#include <stdio.h>

int main()
{int size,count=0,count1=0,key;
printf("size:");
scanf("%d",&size);
int a[size];
printf("key element:");
scanf("%d",&key);
for(int i=0;i<size;i++)
{
    scanf("%d",&a[i]);
}
for(int i=0;i<size;i++)
{
    if(a[i]<key)
    { //printf("\t%d",a[i]);
        count++;}
    
}
printf("\n%d",count);

return 0;
}
