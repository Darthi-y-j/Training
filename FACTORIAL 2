/******************************************************************************
FACTORIAL without USING RECURSION FUNCTION

*******************************************************************************/
#include <stdio.h>

int main()
{ 
    int num,temp=1,i ;
    scanf("%d",&num);
    for (i=1;i<=num;i++)
    {
    temp=temp*i;
    }
    printf("%d",temp);

    return 0;
}
